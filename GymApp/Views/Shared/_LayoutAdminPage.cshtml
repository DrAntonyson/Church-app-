    <link href="~/Content/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" type="text/css" href="https://npmcdn.com/flatpickr/dist/themes/dark.css">
    <script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>
</head>
<body class="skin-blue">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <b class="logo">@Html.ActionLink("Control Panel", "Index", "Admin")</b>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                @*@{
                        var adminInCookie = Request.Cookies["AdminInfo"];
                    }
                    @if (adminInCookie != null)
                    {
                        string email = string.Empty;
                        email = adminInCookie["Email"].ToString();
                        <span style="font-size: 17px; font-weight: bold; margin-left: 50px; color: #fff;">@email</span>
                        <a href="@Url.Action("Index","HomePageTest")" style="float:right; font-size:18px; font-weight: bold; margin-right: 20px; margin-top: 12px;">Food Home</a>
                        <a href="@Url.Action("LogoutAdmin","Admin")" style="float:right; font-size:18px; font-weight: bold; margin-right: 50px; margin-top: 12px;">Logout</a>
                    }
                    else
                    {
                        <a href="@Url.Action("Index","HomePageTest")" style="float:right; font-size:18px; font-weight: bold; margin-right: 20px; margin-top: 12px;">Food Home</a>
                        <a href="@Url.Action("LoginAdmin","Admin")" style="float:right; font-size:18px;font-weight: bold; margin-right: 50px; margin-top: 12px;">Login</a>
                    }*@

                <a>
                    @*@Html.Partial("_LoginPartial")*@
                </a>

            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    @*<div class="pull-left image">
                            <img src="~/Content/dist/img/user.png" class="img-circle" alt="User Image" />
                        </div>*@
                    <div class="pull-left info">
                        <p>@User.Identity.Name</p>
                    </div>
                </div>

                <!-- sidebar menu: : style can be found in sidebar.less -->
