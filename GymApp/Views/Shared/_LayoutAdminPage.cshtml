                                <ul class="treeview-menu">
                                    <li><a href="/Driver/NewDriver"><i class="fa fa-circle-o"></i> Add Driver</a></li>
                                    <li><a href="/Driver/Index"><i class="fa fa-circle-o"></i> Driver List</a></li>
                                </ul>
                            </li>
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-laptop"></i>
                                    <span>Pastor</span>
                                    <i class="fa fa-angle-left pull-right"></i>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="/Trainer/AddTrainer"><i class="fa fa-circle-o"></i> Add Pastor</a></li>
                                    <li><a href="/Trainer/Trainers"><i class="fa fa-circle-o"></i> Pastors</a></li>
                                </ul>
                            </li>
