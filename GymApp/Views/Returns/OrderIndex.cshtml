@model IEnumerable<GymApp.Models.Order>

@{
    ViewBag.Title = "OrderIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>OrderIndex</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.prodcts.ProductName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qty)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Order_Date)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.IsReceived)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.prodcts.ProductName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Qty)
        </td>
     
        <td>
            @Html.DisplayFor(modelItem => item.Order_Date)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.IsReceived)
        </td>
       
        <td>
            @Html.ActionLink("Return Item", "Create", new { id = item.Id }) |
           
           
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
